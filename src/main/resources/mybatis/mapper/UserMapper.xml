<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "_//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.sessprac.user.UserMapper">
    <insert id="joinUser">
        insert into
            USER
        values(
               #{userId},
               #{userName},
               #{userPasswd}
              )
    </insert>
</mapper>